    {
      "name": "3d5e1925",
      "displayName": "movies_kpi",
      "queryLines": [
        "SELECT \r\n",
        "  total_movies,\r\n",
        "  ROUND(avg_rating, 2) AS avg_rating,\r\n",
        "  ROUND(avg_runtime, 1) AS avg_runtime\r\n",
        "FROM gold_movies.movies_kpi;"
      ],
      "catalog": "golden",
      "schema": "gold_movies"
    },
    {
      "name": "30da555c",
      "displayName": "movies_by_genre",
      "queryLines": [
        "SELECT \r\n",
        "  genres,\r\n",
        "  movies_count,\r\n",
        "  ROUND(avg_rating, 2) AS avg_rating\r\n",
        "FROM gold_movies.movies_by_genre\r\n",
        "ORDER BY movies_count DESC;"
      ],
      "catalog": "golden",
      "schema": "gold_movies"
    },
    {
      "name": "ad26ea8d",
      "displayName": "movies_by_year",
      "queryLines": [
        "SELECT * FROM golden.gold_movies.movies_by_year"
      ],
      "catalog": "golden",
      "schema": "gold_movies"
    },
    {
      "name": "3a055f3f",
      "displayName": "top_rated_movies",
      "queryLines": [
        "SELECT \r\n",
        "  title,\r\n",
        "  rating\r\n",
        "FROM gold_movies.top_rated_movies\r\n",
        "ORDER BY rating DESC\r\n",
        "LIMIT 10;"
      ],
      "catalog": "golden",
      "schema": "gold_movies"
    }
